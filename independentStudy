package independentStudy;

import java.util.ArrayList;
import java.util.Scanner;

public class Schedule
{
	private ArrayList<String>schedule;
	private ArrayList<String>newTask;
	private ArrayList<Integer>importance;
	private ArrayList<Integer>date;
	private String whatToAdd;
	
	public Schedule() 
	{
		schedule = new ArrayList<String>();
		newTask =  new ArrayList<String>();
		importance = new ArrayList<Integer>(100);
		date = new ArrayList<Integer>();
		whatToAdd = "";
	}
	public Schedule(ArrayList<String>schedule, ArrayList<String>newTask, ArrayList<Integer>importance, ArrayList<Integer>date, String whatToAdd) 
	{
		this.schedule = schedule;
		this.newTask = newTask;
		this.importance = importance;
		this.date = date;
		this.whatToAdd = whatToAdd;
	}
	public ArrayList<String> getList()
	{
		return this.schedule;
	}
	public void setList(ArrayList<String>schedule) 
	{
		this.schedule = schedule;
	}
	public int getDate() {
		int date1 = 0;
		for(int i = 0; i<date.size(); i++) {
			date1 = date.get(i);
		}
		return date1;
	}
	public void setDate(ArrayList<Integer>date) {
		this.date = date;
		}
	public ArrayList<Integer>getImportance(){
		return this.importance;
	}
	public void setImportance(ArrayList<Integer>importance) {
		this.importance = importance;
	}
	public ArrayList<String> addAssignment(String whatToAdd, ArrayList<String>schedule) 
	{
		ArrayList<String>returned = new ArrayList<String>(schedule);
		System.out.println("Hey! What do you want to add today?");
		Scanner scan = new Scanner(System.in);
		String user = scan.nextLine();
		System.out.println("But before we begin, please place the importance of your assignment from 1-100.");
		int importa = scan.nextInt();
		String update = whatToAdd;
		for (int i = 0; i<returned.size(); i++) {
			if(user.equals("test")) {
				schedule.add(importa, user);
				schedule.add(importa, "TEST");
			} else if(user.equals("quiz")) {
				schedule.add(importa, user);
				schedule.add(importa, "QUIZ");
			} else if(user.equals("project")) {
				schedule.add(importa, user);
				schedule.add(importa, "PROJECT");
			} else if(user.equals("homework")) {
				schedule.add(importa, user);
				schedule.add(importa, "HOMEWORK");
			} else {
				System.out.println("At this time you can only enter the following: \n 1. test \n 2. quiz \n 3. project \n 4. homework");
			}		
		}
		System.out.println("Congrats! It has been added!");
		return returned;
	}
	public void getMySchedule(ArrayList<String>schedule, ArrayList<Integer>date){
		 ArrayList<ArrayList<String> > aList = new ArrayList<ArrayList<String>>(schedule.size()); 
		int count = 1;
		 for(int i = 0; i<aList.size(); i++) {
			for (int j = 0; j<aList.size(); i++) {
				 System.out.print("Assignment Number: " + count + " " + aList.get(i).get(j) + " "); 
			}
			System.out.println();
		}
	}
}
