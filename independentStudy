package independentStudy;

import java.awt.Graphics;
import java.util.ArrayList;
import java.util.Scanner;

public class ToDoList extends Schedule
{
	public static void main(String[] args) {
		System.out.println("Welcome to your daily list!");
		System.out.println("Here you will find what you need to do for the day!");
		
		Scanner scan = new Scanner(System.in);
		
		
	}
	
	
	
	
	
	
	public ToDoList() 
	{
		private ArrayList<String>list;
		private int day;
		public ArrayList<String>needToAdd;
		
		public ToDoList {
			list = new ArrayList<String>();
			day = 0; 
			needToAdd = new ArrayList<String>();
		}
		public ToDoList(ArrayList<String>list, int day, ArrayList<String>needToAdd) {
			this.list = list;
			this.day = day; 
			this.needToAdd = needToAdd;
		}
		public String getList() {
			String forTheDay = "";
			for (int i = 0; i<list.size(); i++) {
				if (list.get(i) == day) {
					list.get(i) = forTheDay;
				}
			}
			return forTheDay;
		}
		public static void setList(ArrayList<String>list) {
			this.list = list;
		}
		public static void (int day) {
			this.day = day;
		}
		public void addToDoList(ArrayList<String>list, ArrayList<String>needToAdd) {
			ArrayList<String>updatedList = new ArrayList<String>();
			for (int i = 0; i<list.size(); i++) {
				if (list.get(i) == null) {
					list.get(i+1).equals(list.get(i));
					updatedList.get(i).equals(list.get(i));
				}
				for (int i = 0; i<needToAdd.size(); i++) {
					if (list.get(i) == null)
					{
						list.get(i).equals(needToAdd.get(i));
						updatedList.get(i).equals(list.get(i));
					}
				}
			}
		}
		public ArrayList<String> completed(ArrayList<String>list){
			super(ArrayList<String>updatedList);
		}
		
		
		
	}

}
